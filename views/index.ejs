<%- include('./partials/head.ejs') %>
<body>
  <%- include('./partials/navbar.ejs') %>
  <h1>Blog Newsfeed</h1>
    <% if (blogData.length > 0) { %>
      <% blogData.forEach(blog => { %>
      <h4><a href='/blog/<%= blog._id %>'><%= blog.title %></a></h4>
      <% }) %>  
    <% } else{ %>
      <h4>No Blogs to Read Yet!</h4>
    <% } %>
</body>
</html>

